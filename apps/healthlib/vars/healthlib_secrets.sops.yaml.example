# Example secrets file for healthlib
# Copy to healthlib_secrets.sops.yaml and encrypt with: sops -e -i healthlib_secrets.sops.yaml
#
# Garmin credentials (for unofficial API via garminconnect/garth)
garmin:
  email: "your-garmin-email@example.com"
  password: "your-garmin-password"

# Strava OAuth credentials (from https://www.strava.com/settings/api)
strava:
  client_id: "your-client-id"
  client_secret: "your-client-secret"
  # These are populated by 'healthlib auth strava' command after OAuth flow
  access_token: ""
  refresh_token: ""
  expires_at: 0
